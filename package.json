{
  "name": "ravagepdf",
  "version": "0.1.3",
  "description": "PDF generation from OpenAPI / Swagger Spec",
  "keywords": [
    "OpenAPI",
    "OpenAPI Specification",
    "Swagger",
    "JSON-Schema",
    "API",
    "REST",
    "documentation",
    "Custom Element",
    "openapi2pdf",
    "swagger",
    "swagger ui",
    "pdf",
    "swagger pdf",
    "openapi pdf"
  ],
  "scripts": {
    "predeploy": "npm run build && npm run build-docs",
    "deploy": "gh-pages -d docsDist",
    "clean": "gh-pages-clean",
    "test": "jest --config=jest.config.json",
    "lint": "eslint .",
    "lint-fix": "eslint --config ./.eslintrc.json --fix .",
    "build": "tsc",
    "start": "node dist/bin/cli.js",
    "build-docs": "typedoc --out docsDist ./lib/**/*"
  },
  "bin": {
    "ravagepdf": "dist/bin/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MBHuman/ravagepdf.git"
  },
  "author": "Evgeny Dmitirev zenka712@mail.ru",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/MBHuman/ravagepdf/issues"
  },
  "homepage": "https://github.com/MBHuman/ravagepdf#readme",
  "devDependencies": {
    "@types/html-to-pdfmake": "^2.4.0",
    "@types/jest": "^29.5.0",
    "@types/jsdom": "^21.1.1",
    "@types/marked": "^4.0.8",
    "@types/pdfkit": "^0.12.9",
    "@types/pdfmake": "^0.2.2",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "gh-pages": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.6",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@readme/openapi-parser": "^2.5.0",
    "@types/yargs": "^17.0.24",
    "fs": "^0.0.1-security",
    "html-to-pdfmake": "^2.4.18",
    "jsdom": "^21.1.1",
    "marked": "^4.3.0",
    "openapi-types": "^12.1.0",
    "pdfkit": "^0.13.0",
    "pdfmake": "^0.2.2",
    "yargs": "^17.0.24"
  }
}
